{
  "name": "faboginger",
  "version": "1.0.4",
  "description": "FaboGinger - 激光切割余料再利用排版工具",
  "main": "dist/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:react\" \"npm run dev:electron\"",
    "dev:react": "vite",
    "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:react && npm run build:electron",
    "build:react": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:win": "npm run build && electron-builder --win",
    "build:win:dir": "npm run build && electron-builder --win --dir",
    "build:win:portable": "npm run build && electron-builder --win portable",
    "build:win:all": "npm run build && electron-builder --win --x64 --ia32",
    "preview": "vite preview"
  },
  "keywords": [
    "laser-cutting",
    "material-reuse",
    "nesting",
    "deepnest"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-icon-maker": "^0.0.5",
    "sharp": "^0.34.5",
    "to-ico": "^1.1.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "potrace": "^2.1.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.3.5"
  }
}
